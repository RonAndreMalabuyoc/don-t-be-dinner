script/source = "extends CharacterBody2D

# ─── SIGNALS ───
signal health_changed(current_hp, max_hp)

# ─── CONFIGURATION ───
const HP_PER_HEART := 10  # Important: 1 Heart visual = 10 HP actual
@export var base_hearts := 3

# ───────── MOVEMENT ─────────
@export var speed := 400
@export var jump_force := -600
@export var gravity := 1200
@export var hurt_knockback_damp := 1800.0
var jump_anim_requested := false

# ───────── DASH ─────────
@export var dash_speed := 900.0
@export var dash_duration := 0.12
@export var dash_cooldown := 1.5
var is_dodging := false

# ───────── SHOOTING ─────────
@export var shoot_cooldown := 0.3
@export var bullet_scene: PackedScene
const SHOOT_FRAME := 3
var shoot_direction: Vector2 = Vector2.RIGHT
var is_shooting := false
@export var base_damage := 10  # default player damage

# ───────── FRUIT SHOOTING ─────────
@export var fruit_shoot_cooldown := 0.3
@export var fruit_projectiles := {
	\"Pomegranate\": preload(\"res://scenes/Bullets/PomegranateBullet_Throw.tscn\"),
	\"Orange\": preload(\"res://scenes/Bullets/OrangeBullet_Throw.tscn\"),
	\"Banana\": preload(\"res://scenes/Bullets/BananaBullet_Throw.tscn\"),
	\"Coconut\": preload(\"res://scenes/Bullets/CoconutBullet_Throw.tscn\"),
}

# ───────── HEALTH ─────────
# NOTE: We calculate max_health in _ready based on hearts
var max_health := 30 
var current_health := 30

@export var hurt_lock_time := 0.25
@export var invulnerability_time := 1.3
@export var blink_interval := 0.1
var is_invulnerable := false
var is_dead := false
var can_take_damage := true
const INVINCIBILITY_TIME := 1.3

# ───────── NODES ─────────
@onready var sprite: AnimatedSprite2D = $AnimatedSprite2D
@onready var shoot_point: Node2D = $ShootPoint
@onready var hitbox: Area2D = $Hitbox
@onready var camera: Camera2D = $Camera2D
@onready var fruit_ui_current: Label = get_tree().current_scene.get_node(\"HUD/FruitUI/Current_fruit\")
@onready var fruit_ui_reserve: Label = get_tree().current_scene.get_node_or_null(\"HUD/FruitUI/Reserve_fruit\")

# ───────── SFX ─────────
@onready var quack_player = $QuackPlayer
@onready var hurt_player = $HurtPlayer
@onready var shoot_player = $ShootPlayer
@onready var dash_player = $DashPlayer
@onready var land_player = $LandPlayer
@onready var walk_player = $WalkPlayer
@onready var pickup_player = $PickupPlayer

# ───────── STATE ─────────
var facing_right := true
var is_dashing := false
var can_dash := true
var dash_time_left := 0.0
var dash_dir := 0.0
var is_hurt := false
const HURT_ANIM_TIME := 1.0
var hurt_dir := 0
var jumps_done := 0
var max_jumps := 1
var can_shoot := true
var is_emoting := false
var shot_fired_this_anim := false

var can_shoot_fruit := true
var quack_timer := 0.0

# ───────── READY ─────────
func _ready():
	Global.node_creation_parent = self
	Global.playerbody = self
	SkillManager.apply_existing_skills_to_player()
	Global.player_alive = true
	
	max_health = base_hearts * HP_PER_HEART
	current_health = max_health
	
	# ─── 2. NOTIFY UI ───
	# Wait one frame to ensure UI is ready to receive signal
	await get_tree().process_frame
	health_changed.emit(current_health, max_health)

	# ─── 3. SKILLS ───
	if SkillManager.swift_feet_active: apply_swift_feet()
	if SkillManager.double_jump_active: apply_double_jump()
	if SkillManager.sharp_blows_active: apply_sharp_blows()
	if SkillManager.quick_recovery_active: apply_quick_recovery()
	if SkillManager.dash_mastery_active: apply_dash_mastery()

	# ─── 4. CONNECTIONS ───
	hitbox.body_entered.connect(_on_hitbox_body_entered)
	hitbox.area_entered.connect(_on_hitbox_area_entered)
	sprite.frame_changed.connect(_on_sprite_frame_changed)
	
	print(\"PLAYER READY:\", get_path())
	_debug_slots(\"Ready\")
	_update_fruit_ui()
	
	quack_player.play()
	quack_timer = randf_range(6.0, 14.0)

	if SkillManager.dash_mastery_active:
		dash_speed *= 1.25
		dash_cooldown *= 0.7

# ───────── SKILL FUNCTIONS ─────────
func apply_dash_mastery():
	if SkillManager.dash_mastery_active:
		dash_speed *= 1.25
		dash_cooldown *= 0.7

func apply_double_jump():
	max_jumps = 2
	print(\"Double Jump unlocked! Max jumps:\", max_jumps)

# ───────── PHYSICS ─────────
func _physics_process(delta):
	# HURT LOCK
	if is_hurt:
		velocity.y += gravity * delta
		velocity.x = move_toward(velocity.x, 0, hurt_knockback_damp * delta)
		if is_on_floor():
			velocity.x = move_toward(velocity.x, 0, hurt_knockback_damp * 2 * delta)
	
	if is_on_floor():
		jumps_done = 0

	# Jump
	if Input.is_action_just_pressed(\"jump\") and not is_emoting:
		if jumps_done < max_jumps:
			velocity.y = jump_force
			jumps_done += 1
			jump_anim_requested = true
		move_and_slide()
		return

	# Shoot
	if Input.is_action_pressed(\"shoot\") and can_shoot and not is_emoting and not is_hurt:
		start_shoot()

	if Input.is_action_just_pressed(\"dance\"):
		is_emoting = !is_emoting

	# Fruit
	if Input.is_action_just_pressed(\"fruit_shoot\") and not is_emoting and not is_hurt:
		shoot_fruit()

	if Input.is_action_just_pressed(\"swap_fruit\") and not is_emoting and not is_hurt:
		swap_fruit_slots()

	if is_dead: return

	# Dance
	if is_emoting:
		velocity.x = 0
		handle_animations()
		move_and_slide()
		return

	# Gravity
	if not is_on_floor() and not is_dashing:
		velocity.y += gravity * delta

	# Dash
	if Input.is_action_just_pressed(\"dash\") and can_dash and not is_emoting and not is_dashing:
		start_dash()

	if is_dashing:
		dash_time_left -= delta
		velocity.x = dash_dir * dash_speed
		velocity.y = 0
		if dash_time_left <= 0:
			stop_dash()
	else:
		var direction := Input.get_axis(\"Left\", \"Right\")
		if direction != 0 and not is_emoting:
			velocity.x = direction * speed
			facing_right = direction > 0
		else:
			velocity.x = move_toward(velocity.x, 0, 40)

		if Input.is_action_just_pressed(\"jump\") and is_on_floor() and not is_emoting:
			velocity.y = jump_force
			jumps_done += 1
			
		if is_on_floor():
			jumps_done = 0

	handle_animations()
	sprite.flip_h = not facing_right
	move_and_slide()

	quack_timer -= delta
	if quack_timer <= 0.0:
		quack_player.play()
		quack_timer = randf_range(6.0, 14.0)
		
	var was_on_floor := false
	if not was_on_floor and is_on_floor():
		land_player.play()
	was_on_floor = is_on_floor()
	
	if is_on_floor() and abs(velocity.x) > 10:
		if not walk_player.playing: walk_player.play()
	else:
		if walk_player.playing: walk_player.stop()

func get_shoot_direction() -> Vector2:
	var mouse_pos = get_global_mouse_position()
	var dir = (mouse_pos - shoot_point.global_position).normalized()
	return dir

# ───────── DASH ─────────
func start_dash():
	can_dash = false
	is_dashing = true
	is_dodging = true
	is_invulnerable = true
	can_take_damage = false
	hitbox.monitoring = false
	dash_time_left = dash_duration

	var input_dir := Input.get_axis(\"Left\", \"Right\")
	if input_dir != 0:
		dash_dir = sign(input_dir)
	else:
		dash_dir = 1 if facing_right else -1
	facing_right = dash_dir > 0

	await get_tree().create_timer(dash_cooldown).timeout
	can_dash = true
	dash_player.play()

func stop_dash():
	is_dashing = false
	is_dodging = false
	is_invulnerable = false
	can_take_damage = true
	hitbox.monitoring = true
	velocity.x = move_toward(velocity.x, 0, 200)
	sprite.modulate = Color.WHITE

# ───────── SHOOTING ─────────
# ───────── SHOOTING ─────────
func shoot():
	var bullet := Global.instance_node(
		bullet_scene,
		shoot_point.global_position,
		Global.node_creation_parent
	) as Bullet

	if bullet == null:
		return

	# If your Bullet script expects a direction:
	var mouse_pos := get_global_mouse_position()
	bullet.direction = (mouse_pos - shoot_point.global_position).normalized()

	# If you have SFX:
	if has_node(\"ShootPlayer\"):
		$ShootPlayer.play()


func _on_sprite_frame_changed():
	if sprite.animation != \"shoot\": return
	if sprite.frame == SHOOT_FRAME and not shot_fired_this_anim:
		shot_fired_this_anim = true
		if bullet_scene and Global.node_creation_parent:
			var bullet = Global.instance_node(bullet_scene, shoot_point.global_position, Global.node_creation_parent) as Bullet
			bullet.direction = get_shoot_direction()
		if \"damage\" in bullet_scene:
			bullet_scene.damage = base_damage
			
func start_shoot():
	if not can_shoot: return
	can_shoot = false
	is_shooting = true
	shot_fired_this_anim = false
	facing_right = get_global_mouse_position().x > global_position.x
	sprite.play(\"shoot\")
	await sprite.animation_finished
	is_shooting = false
	await get_tree().create_timer(shoot_cooldown).timeout
	can_shoot = true

# ───────── POWERUPS ─────────
func apply_swift_feet():
	speed *= 1
	print(\"Swift Feet applied! New speed:\", speed)
	
func apply_sharp_blows():
	base_damage = int(base_damage * 1.5)
	print(\"Sharp Blows applied! New damage:\", base_damage)

func apply_quick_recovery():
	hurt_lock_time *= 0.5
	invulnerability_time *= 0.5
	print(\"Quick Recovery applied! Hurt lock:\", hurt_lock_time)

# ───────── FRUIT SHOOTING ─────────
var _fruit_shot_seq := 0

func shoot_fruit() -> void:
	_fruit_shot_seq += 1
	var seq := _fruit_shot_seq
	if not can_shoot_fruit: return
	if current_fruit == \"\": return

	var fruit_id := current_fruit
	if not fruit_projectiles.has(fruit_id) or fruit_projectiles[fruit_id] == null: return

	can_shoot_fruit = false
	var proj_scene: PackedScene = fruit_projectiles[fruit_id]
	var proj := Global.instance_node(proj_scene, shoot_point.global_position, Global.node_creation_parent)
	var dir := get_shoot_direction()
	if proj != null:
		if \"direction\" in proj: proj.direction = dir
		elif proj.has_method(\"setup\"): proj.call(\"setup\", dir)

	is_shooting = true
	sprite.play(\"shoot\")
	await sprite.animation_finished
	is_shooting = false

	if seq != _fruit_shot_seq: return
	current_fruit = \"\"
	if reserve_fruit != \"\":
		current_fruit = reserve_fruit
		reserve_fruit = \"\"
	_update_fruit_ui()
	await get_tree().create_timer(fruit_shoot_cooldown).timeout
	if seq != _fruit_shot_seq: return
	can_shoot_fruit = true
	
func _on_timer_timeout():
	can_shoot = true

# ───────── ANIMATIONS ─────────
func handle_animations():
	if is_hurt: return
	if Input.is_action_pressed(\"shoot\"):
		var mouse_x := get_global_mouse_position().x
		facing_right = mouse_x > global_position.x
	if is_dead: return
	if is_dashing:
		sprite.modulate = Color(1, 1, 1, 0.6)
		sprite.play(\"dash\" if sprite.sprite_frames.has_animation(\"dash\") else \"walk\")
		return
	if is_emoting:
		if sprite.animation != \"dance\": sprite.play(\"dance\")
		return

	if not is_on_floor():
		if jump_anim_requested:
			sprite.stop()
			sprite.play(\"jump\")
			jump_anim_requested = false
			return
		if velocity.y < 0:
			if sprite.animation != \"jump\": sprite.play(\"jump\")
		else:
			if sprite.sprite_frames.has_animation(\"fall\") and sprite.animation != \"fall\":
				sprite.play(\"fall\")
		return

	if is_shooting: return
	if abs(velocity.x) > 10: sprite.play(\"walk\")
	else: sprite.play(\"idle\")

# ───────── DAMAGE & HEALING ─────────
func _on_hitbox_body_entered(body):
	if is_dead or not can_take_damage: return
	if body.is_in_group(\"Enemy\") and \"damage_amount\" in body:
		hurt_dir = sign(global_position.x - body.global_position.x)
		take_damage(body.damage_amount) # Ensure Enemy deals 10 damage!

func _on_hitbox_area_entered(area):
	if is_dead or not can_take_damage: return
	if area.is_in_group(\"EnemyDamage\"):
		hurt_dir = sign(global_position.x - area.global_position.x)
		take_damage(10) # Area damage default = 10 (1 Heart)

func take_damage(amount: int):
	if is_dead or is_hurt or is_invulnerable: return

	current_health -= amount
	health_changed.emit(current_health, max_health)

	if current_health <= 0:
		die()
		return
	# Tell Global to subtract health. 
	# Global.take_damage() will trigger die() if health hits 0.
	Global.take_damage(amount)

	start_hurt_and_invincibility()

# ─── WAVE RECOVERY LOGIC ───
# CALL THIS from your World/WaveManager script when a wave ends!
func on_wave_completed():
	if is_dead: return
	
	if \"wave_recovery_active\" in SkillManager and SkillManager.wave_recovery_active:
		# Heal 1 Heart (10 HP)
		heal(HP_PER_HEART)
		print(\"Wave Complete: +1 Heart Recovered\")

func heal(amount: int):
	# Don't heal if already full
	if current_health >= max_health: return
	
	current_health += amount
	
	# Cap at Max
	if current_health > max_health:
		current_health = max_health

	print(\"Healed! HP:\", current_health, \"/\", max_health)
	hurt_player.play() # Reusing hurt sound (or change to pickup_player)
	
	# Notify UI
	health_changed.emit(current_health, max_health)

func heal_after_wave(amount: int):
	# Wrapper for compatibility if other scripts call this
	heal(amount)

# ───────── HIT RESPONSE ─────────
func start_hurt_and_invincibility():
	is_hurt = true
	is_invulnerable = true
	can_take_damage = false
	hitbox.monitoring = false
	facing_right = hurt_dir < 0
	sprite.flip_h = not facing_right
	velocity.x = hurt_dir * 320
	velocity.y = -220
	sprite.stop()
	sprite.play(\"hurt\")
	await sprite.animation_finished
	is_hurt = false
	start_blinking()

func start_blinking():
	var elapsed := 0.0
	while elapsed < invulnerability_time:
		sprite.visible = false
		await get_tree().create_timer(blink_interval).timeout
		sprite.visible = true
		await get_tree().create_timer(blink_interval).timeout
		elapsed += blink_interval * 2
	end_invulnerability()

func end_invulnerability():
	is_invulnerable = false
	can_take_damage = true
	hitbox.monitoring = true
	sprite.visible = true

# ───────── DEATH ─────────
func die():
	if is_dead: return
	is_dead = true
	Global.player_alive = false
	sprite.play(\"death\")
	sprite.modulate = Color.WHITE
	if camera: create_tween().tween_property(camera, \"zoom\", Vector2(3, 3), 0.5)
	await get_tree().create_timer(3.0).timeout
	queue_free()
	
	# 1. STOP PHYSICS (Prevents the player from falling into the killzone again)
	set_physics_process(false)
	set_process(false)
	if hitbox:
		hitbox.monitoring = false
	
	# 2. VISUALS
	sprite.play(\"hurt\")
	
	# 3. CAMERA & EFFECTS (Set to keep working even if we pause)
	if camera:
		var cam_tween = create_tween().set_pause_mode(Tween.TWEEN_PAUSE_PROCESS)
		cam_tween.tween_property(camera, \"zoom\", Vector2(3, 3), 0.5)

	var tween = create_tween().set_parallel(true).set_pause_mode(Tween.TWEEN_PAUSE_PROCESS)
	tween.tween_property(sprite, \"modulate\", Color(1, 0, 0, 0), 1.5)
	tween.tween_property(sprite, \"rotation_degrees\", 360, 1.0)

	# 4. PAUSE THE GAME WORLD (Stops enemies/timers)
	get_tree().paused = true

	# 5. WAIT (Using real time so it works while paused)
	await get_tree().create_timer(2.0).timeout
	
	# 6. LOAD DEATH SCREEN
	var death_screen_path = \"res://scenes/UI/death_screen.tscn\"
	var death_scene = load(death_screen_path)
	
	if death_scene:
		var instance = death_scene.instantiate()
		# Add to root so it's not zoomed by the camera
		get_tree().root.add_child(instance) 
		Input.set_mouse_mode(Input.MOUSE_MODE_VISIBLE)
	else:
		print(\"CRITICAL ERROR: Could not find death_screen.tscn at: \", death_screen_path)
		get_tree().paused = false
		get_tree().reload_current_scene()

# ───────── FRUIT SLOTS ─────────
var current_fruit: String = \"\"
var reserve_fruit: String = \"\"

func _debug_slots(context: String) -> void:
	print(\"[\" + context + \"] Current:\", current_fruit, \" | Reserve:\", reserve_fruit)

func push_powerup(item_id: String, _duration: float = -1.0, auto_fire: bool = false) -> void:
	if current_fruit == \"\": current_fruit = item_id
	elif reserve_fruit == \"\": reserve_fruit = item_id
	else: reserve_fruit = item_id
	_update_fruit_ui()

func swap_fruit_slots() -> void:
	if current_fruit == \"\" and reserve_fruit == \"\": return
	var tmp := current_fruit
	current_fruit = reserve_fruit
	reserve_fruit = tmp
	_update_fruit_ui()

func _fruit_name_or_empty(v: String) -> String:
	return v if v != \"\" else \"(empty)\"

func _update_fruit_ui() -> void:
	if is_instance_valid(fruit_ui_current):
		fruit_ui_current.text = \"Current: \" + _fruit_name_or_empty(current_fruit)
	if is_instance_valid(fruit_ui_reserve):
		fruit_ui_reserve.text = \"Reserve: \" + _fruit_name_or_empty(reserve_fruit)
# ───────── LIVE UPDATES ─────────
# Call this function immediately after you unlock the \"+1 Heart\" skill!
# In Player.gd

func update_max_health():
	max_health += HP_PER_HEART
	# choose ONE behavior:

	# OPTION A: fill new heart
	current_health += HP_PER_HEART

	# OPTION B: leave new heart empty
	# current_health = min(current_health, max_health)

	current_health = clamp(current_health, 0, max_health)
	health_changed.emit(current_health, max_health)
		
func _on_exitbutton_pressed() -> void:
	var sfx = $ButtonManager/exit_button/SFX_button3
	sfx.play()  # play the quit sound effect
	
	await sfx.finished
	
	get_tree().quit()
"

